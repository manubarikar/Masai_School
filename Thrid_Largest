function thirdlargest(num1,num2,num3)
{
let largest
if (num1 > num2) {
    if (num1 > num3) {
        largest = num1
    } else {
        largest = num3
    }
} else {
    if (num2 > num3) {
        largest = num2
    } else {
        largest = num3
    }
}
 return(largest + " (The largest number)")
}
console.log(thirdlargest(38,78,20))
